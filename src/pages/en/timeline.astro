---
import Layout from '../../layouts/Layout.en.astro';
---

<Layout title="Timeline">
    <div class="hero">
        <div class="hero-content">
            <h1>Project Timeline</h1>
            <p class="subtitle">Key milestones and events in our journey</p>
        </div>
    </div>

    <div class="main-content">
        <section class="timeline">
            <div class="timeline-entry">
                <h3>Milestone 1</h3>
                <p>Description of milestone 1.</p>
            </div>
            <div class="timeline-entry">
                <h3>Milestone 2</h3>
                <p>Description of milestone 2.</p>
            </div>
            
        </section>
    </div>

    <section class="cta-section">
        <div class="cta-content">
            <h2>Want to Know More?</h2>
            <p>Contact us for more information about our project.</p>
        </div>
    </section>
</Layout>